<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="com.organ.action.abutment" extends="all">
		<!-- 成员管理 -->
		<action name="abmember" class="com.organ.action.abutment.AbutmentMemberAction">
			<result name="text" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
		<!-- 权限管理  -->
		<action name="abprivilege" class="com.organ.action.abutment.AbutmentPrivilegeAction">
			<result name="text" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
		<!-- 授权管理  -->
		<action name="abauth" class="com.organ.action.abutment.AbutmentAuthAction">
			<result name="text" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
		<action name="abranch" class="com.organ.action.abutment.AbutmentBranchAction">
			<result name="text" type="stream">
				<param name="contentType">text/plain</param>
				<param name="inputName">inputStream</param>
			</result>
		</action>
	</package>

</struts>

