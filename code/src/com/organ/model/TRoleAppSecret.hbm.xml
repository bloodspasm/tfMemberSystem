<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="com.organ.model.TRoleAppSecret" table="t_role_appsecret" catalog="organ">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="roleId" type="java.lang.Integer">
			<column name="role_id" not-null="true"></column>
		</property>
		<property name="appsecretId" type="java.lang.Integer">
            <column name="appsecret_id" not-null="true" />
        </property>
	</class>
</hibernate-mapping>