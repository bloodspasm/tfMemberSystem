		<action name="auth" class="com.organ.action.auth.AppSecretAction">
			<!--  服务器做重定向 -->
			<!--
				<result name="redirectLogin" type="redirectAction"> <param
				name="actionName">auth!redirectLogin</param> <param
				name="unAuthToken">${unAuthToken}</param> </result>
			-->
			
			<result name="oaLogin">/page/web/deng.jsp</result>
